<launch>

<node name = "pd_server" pkg = "pd_controller" type = "pd_server" output = "screen">

<rosparam file = "$(find pd_controller)/param/local_costmap_params.yaml" command = "load"/>

</node>

<node name = "tf_tree" pkg = "setup_tf_tree" type = "setup_tf_tree_node" />  

<arg name="map_file" default="$(find res_rob_navigation)/maps/empty_garage.yaml"/>
  <node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)" />

  <arg name="initial_pose_x" default="0.0"/> <!-- Use 17.0 for willow's map in simulation -->
  <arg name="initial_pose_y" default="0.0"/> <!-- Use 17.0 for willow's map in simulation -->
  <arg name="initial_pose_a" default="0.0"/>
  <include file="$(find res_rob_navigation)/launch/includes/amcl.launch.xml">
    <arg name="initial_pose_x" value="0"/>
    <arg name="initial_pose_y" value="0"/>
    <arg name="initial_pose_a" value="0"/>
     
  </include>


</launch>
